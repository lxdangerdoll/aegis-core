          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "refId": "A",
            "datasource": {
              "type": "influxdb",
              "uid": "influxdb"
            },
            "query": "from(bucket: \"whisperengine\")\n  |> range(start: -7d)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"vector_classification\")\n  |> filter(fn: (r) => r[\"_field\"] == \"confidence\")\n  |> group(columns: [\"primary_vector\"])\n  |> count()\n  |> group()\n  |> rename(columns: {_value: \"count\", primary_vector: \"vector_type\"})"
          }
        ],
        "options": {
          "pieType": "pie",
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
