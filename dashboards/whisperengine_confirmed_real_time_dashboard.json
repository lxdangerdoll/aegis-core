  "panels": [
    {
      "id": 1,
      "title": "Active Sessions (LIVE)",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -30m)\n  |> filter(fn: (r) => r._measurement == \"response_time_v2\")\n  |> filter(fn: (r) => r._field == \"active_sessions\")\n  |> last()",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 100
              },
              {
                "color": "red",
                "value": 200
              }
