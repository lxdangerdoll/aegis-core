{
  "id": null,
  "title": "WhisperEngine Character Performance Overview",
  "tags": [
    "whisperengine",
    "character-intelligence",
    "performance"
  ],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "panels": [
    {
      "id": 1,
      "title": "Character Response Times",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._measurement == \"response_time_v2\")\n  |> filter(fn: (r) => r._field == \"response_length\")\n  |> aggregateWindow(every: 2m, fn: mean, createEmpty: false)",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line"
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Knowledge Query Success Rate",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._measurement == \"user_emotion\")\n  |> filter(fn: (r) => r._field == \"confidence\")\n  |> aggregateWindow(every: 2m, fn: mean, createEmpty: false)",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "green",
                "value": 90
              }
            ]
          }
        }
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "5s"
}