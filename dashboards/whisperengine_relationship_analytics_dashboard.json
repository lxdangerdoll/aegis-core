{
  "id": null,
  "title": "WhisperEngine Relationship Analytics",
  "tags": [
    "whisperengine",
    "relationships",
    "engagement"
  ],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Trust Levels Over Time",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -6h)\n  |> filter(fn: (r) => r._measurement == \"conversation_quality\")\n  |> filter(fn: (r) => r._field == \"engagement_score\")\n  |> aggregateWindow(every: 5m, fn: mean, createEmpty: false)",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          },
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Affection Progression",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -6h)\n  |> filter(fn: (r) => r._measurement == \"conversation_quality\")\n  |> filter(fn: (r) => r._field == \"emotional_resonance\")\n  |> aggregateWindow(every: 5m, fn: mean, createEmpty: false)",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          },
          "color": {
            "mode": "continuous-GrYlRd"
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Current Engagement Score",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._measurement == \"conversation_quality\")\n  |> filter(fn: (r) => r._field == \"engagement_score\")\n  |> last()",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 0.5
              },
              {
                "color": "green",
                "value": 0.8
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Relationship Confidence by Bot",
      "type": "bargauge",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._measurement == \"conversation_quality\")\n  |> filter(fn: (r) => r._field == \"relationship_confidence\")\n  |> group(columns: [\"bot\"])\n  |> mean()",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "color": {
            "mode": "continuous-GrYlRd"
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Communication Comfort Trends",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -6h)\n  |> filter(fn: (r) => r._measurement == \"character_consistency_v2\")\n  |> filter(fn: (r) => r._field == \"communication_comfort\")\n  |> aggregateWindow(every: 10m, fn: mean, createEmpty: false)",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 8
      }
    }
  ]
}