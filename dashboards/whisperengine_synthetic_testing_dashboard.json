{
  "id": null,
  "title": "WhisperEngine Synthetic Testing & Validation",
  "tags": [
    "whisperengine",
    "synthetic",
    "testing",
    "validation"
  ],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-2h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Synthetic Test Quality Score",
      "type": "gauge",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -2h)\n  |> filter(fn: (r) => r._measurement == \"synthetic_test_quality\")\n  |> filter(fn: (r) => r._field == \"value\")\n  |> mean()",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 0.7
              },
              {
                "color": "green",
                "value": 0.9
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Active Synthetic Users",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -2h)\n  |> filter(fn: (r) => r._measurement == \"performance_metrics\")\n  |> filter(fn: (r) => r._field == \"active_synthetic_users\")\n  |> last()",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 25
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Unique Synthetic Users",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -2h)\n  |> filter(fn: (r) => r._measurement == \"performance_metrics\")\n  |> filter(fn: (r) => r._field == \"unique_synthetic_users\")\n  |> last()",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 4
      }
    },
    {
      "id": 4,
      "title": "Test Duration Tracking",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -2h)\n  |> filter(fn: (r) => r._measurement == \"performance_metrics\")\n  |> filter(fn: (r) => r._field == \"test_duration_hours\")\n  |> aggregateWindow(every: 5m, fn: sum, createEmpty: false)",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "h",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Synthetic Conversation Quality",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -2h)\n  |> filter(fn: (r) => r._measurement == \"synthetic_conversation_quality\")\n  |> filter(fn: (r) => r._field == \"value\")\n  |> aggregateWindow(every: 5m, fn: mean, createEmpty: false)",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "id": 6,
      "title": "Synthetic Emotion Testing",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -2h)\n  |> filter(fn: (r) => r._measurement == \"synthetic_emotion_testing\")\n  |> filter(fn: (r) => r._field == \"emotion_detection_performance\")\n  |> aggregateWindow(every: 5m, fn: mean, createEmpty: false)",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 7,
      "title": "Validation Results Summary",
      "type": "table",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"performance_metrics\")\n  |> range(start: -2h)\n  |> filter(fn: (r) => r._measurement == \"synthetic_test_quality\")\n  |> group(columns: [\"bot\"])\n  |> mean()\n  |> yield(name: \"mean\")",
          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-whisperengine"
          }
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 14
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "displayMode": "basic",
            "align": "center"
          }
        }
      }
    }
  ]
}